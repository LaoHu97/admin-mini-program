.header{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  color: #fff;
  font-size: 32rpx;
  background-color: #409EFF;
}
.header_main{
  width: 100%;
}
.header button{
  height: 100rpx;
  line-height: 100rpx;
}
.iconfont{
  margin-right: 5rpx;
}
button[type=primary] {
  color:#fff;
  font-size: 32rpx;
  background-color:#409EFF;
}
button[type=primary]::after {
  border: 0;
}
.header .button-hover{
  color: #cdcdcd;
  background-color: #409EFF;
}
.select_date_title{
  font-size: 34rpx;
  line-height: 100rpx;
  background-color: #F2F6FC;
  display: block;
  padding-left: 20rpx;
}
.select_date_picker{
  width: 240rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  font-size: 28rpx;
  color: #606266;
}
.select_date{
  display: flex;
  justify-content: space-around;
  margin: 20rpx 0;
}
.bottom_main{
  padding: 20rpx;
}
.bottom_main button{
  margin-top: 20rpx;
  font-size: 38rpx;
}
.header_count{
  width: 90%;
  background-color: #fff;
  display: flex;
  justify-content: space-around;
  border-radius: 10rpx;
}
.header_count_left{
  display: flex;
  flex-direction: column;
  padding: 50rpx 0;
  text-align: center;
  line-height: 1.5;
}
.header_count_amount{
  font-size: 34rpx;
  color: #409EFF;
  font-weight: bolder;
}
.header_count_title{
  font-size: 28rpx;
}
.page_list{
  background-color: #fff;
  width: 90%;
  border-radius: 10rpx;
}
.page_list_scroll{
  width: 100%;
}