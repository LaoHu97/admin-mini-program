.setting_page{
  height: 100%;
  color: #303133;
  background-color: #F2F6FC;
}
.page_top{
  width: 100%;
  height: 300rpx;
  background-color: #409EFF;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color: #fff;
  font-size: 32rpx;
  font-weight: bolder;
}
.page_top text{
  font-size: 24rpx;
  font-weight: normal;
}
.page_top_img{
  background-color: #fff;
  border-radius: 500px;
  width: 60rpx;
  height: 60rpx;
  padding: 20rpx;
  margin-bottom: 10rpx;
}
.page_main_list{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 20rpx;
  height: 100rpx;
  background-color: #fff;
}
.page_main_list_left{
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 30rpx;
}
.page_main_list_left .iconfont{
  font-size: 40rpx;
  margin-right: 10rpx;
  color: #fff;
  background-color: #f00;
  padding: 10rpx;
  border-radius: 8rpx;
}
.page_main{
  margin-top: 20rpx;
  border-top: 1rpx solid #DCDFE6;
  border-bottom: 1rpx solid #DCDFE6;
}
.page_main_list_right{
  color: #DCDFE6;
}
.page_main_hover{
  background-color: #DCDFE6;
}